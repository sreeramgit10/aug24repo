{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-manvi08"
		},
		"schedule-9am_properties_dynamic_dataset_trigger_parameters_inputdb": {
			"type": "string",
			"defaultValue": "DB-A"
		},
		"schedule-9am_properties_dynamic_dataset_trigger_parameters_outputdb": {
			"type": "string",
			"defaultValue": "DB-B"
		},
		"schedule-9am_properties_dynamic_dataset_trigger_parameters_inputtable": {
			"type": "string",
			"defaultValue": "tbl-A"
		},
		"schedule-9am_properties_dynamic_dataset_trigger_parameters_outputtable": {
			"type": "string",
			"defaultValue": "tbl-B"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/schedule-9am')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "trigger-schedule",
							"type": "PipelineReference"
						},
						"parameters": {}
					},
					{
						"pipelineReference": {
							"referenceName": "dynamic_dataset_trigger",
							"type": "PipelineReference"
						},
						"parameters": {
							"inputdb": "[parameters('schedule-9am_properties_dynamic_dataset_trigger_parameters_inputdb')]",
							"outputdb": "[parameters('schedule-9am_properties_dynamic_dataset_trigger_parameters_outputdb')]",
							"inputtable": "[parameters('schedule-9am_properties_dynamic_dataset_trigger_parameters_inputtable')]",
							"outputtable": "[parameters('schedule-9am_properties_dynamic_dataset_trigger_parameters_outputtable')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 1,
						"startTime": "2024-08-26T03:44:00",
						"timeZone": "India Standard Time"
					}
				}
			},
			"dependsOn": []
		}
	]
}